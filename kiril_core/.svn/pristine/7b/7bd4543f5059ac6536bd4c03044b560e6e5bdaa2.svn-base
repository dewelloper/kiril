
namespace Markum.Cloud.Libraries.Mail
{
    // When the mailbox is redirected. 
    // 0=Disabled
    // 1=Enabled
    // 2=Redirect and keep a copy of the message in the mailbox
    public static class MailboxRedirectionStatus
    {
        public const long NOTSUPPLIED = -1L;
        public const long DISABLED = 0L;
        public const long ENABLED = 1L;
        public const long KEEPCOPY = 2L;
    }   
}